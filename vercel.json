{
  "redirects": [
    {
      "source": "/localhost:3000/:path*",
      "destination": "https://guardian-solana-shield.vercel.app/",
      "statusCode": 307
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>.+)-guardian-solana-shield.vercel.app"
        }
      ],
      "destination": "https://your-production-domain.vercel.app/:path*",
      "statusCode": 307
    }
  ]
}